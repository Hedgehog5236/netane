        <activity android:name=".HighLowActivity" ></activity>
        <activity android:name=".ThemeActivity" ></activity>
        <activity android:name=".ThemeSettingsActivity" ></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <!--        <meta-data-->
<!--            android:name="preloaded_fonts"-->
<!--            android:resource="@array/preloaded_fonts" />-->
